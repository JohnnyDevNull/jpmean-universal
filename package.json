{
  "name": "jp-mean-universal",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:server": "jest --config projects/nest-server/jest.config.js",
    "test:server-w": "jest --config projects/nest-server/jest.config.js --watch",
    "test:server-cov": "jest --config projects/nest-server/jest.config.js --coverage",
    "lint": "ng lint",
    "lint:server": "tslint -c projects/nest-server/tslint.json -p projects/nest-server/tsconfig.server.json projects/nest-server/src/**/*.ts --force",
    "e2e": "ng e2e",
    "dev:ssr": "ng run universal-app:serve-ssr",
    "serve:ssr": "node dist/jp-mean/server/main.js",
    "build:ssr": "ng build --prod && ng run universal-app:server:production",
    "prerender": "ng run universal-app:prerender",
    "prebuild:ssr": "ngcc",
    "postinstall": "ngcc"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnnyDevNull/jp-mean-universal"
  },
  "dependencies": {
    "@angular/animations": "~9.1.1",
    "@angular/common": "~9.1.1",
    "@angular/compiler": "~9.1.1",
    "@angular/core": "~9.1.1",
    "@angular/forms": "~9.1.1",
    "@angular/platform-browser": "~9.1.1",
    "@angular/platform-browser-dynamic": "~9.1.1",
    "@angular/platform-server": "~9.1.1",
    "@angular/router": "~9.1.1",
    "@angular/service-worker": "~9.1.1",
    "@nestjs/common": "~7.0.7",
    "@nestjs/core": "~7.0.7",
    "@nestjs/ng-universal": "~3.1.0",
    "@nestjs/platform-express": "~7.0.7",
    "@nguniversal/express-engine": "~9.1.0",
    "bootstrap": "^4.4.1",
    "class-transformer": "~0.2.3",
    "class-validator": "~0.9.1",
    "express": "~4.17.1",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~6.5.5",
    "tslib": "~1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.1",
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "~9.1.1",
    "@angular/compiler-cli": "~9.1.1",
    "@angular/language-service": "~9.1.1",
    "@nestjs/testing": "^7.0.7",
    "@nguniversal/builders": "~9.1.0",
    "@nrwl/cypress": "^9.2.2",
    "@nrwl/workspace": "^9.2.2",
    "@types/express": "~4.17.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "codelyzer": "~5.2.2",
    "cypress": "^4.4.0",
    "jest": "^25.3.0",
    "ts-node": "~8.8.2",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  }
}
